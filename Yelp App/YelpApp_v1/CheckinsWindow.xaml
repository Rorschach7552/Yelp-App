<Window x:Class="Application.CheckinsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Application"
        mc:Ignorable="d"
        Title="CheckinsWindow" Height="600" Width="1000">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        <WpfPlot Name="plot"/>
        <Button Content="Checkin" HorizontalAlignment="Left" VerticalAlignment="Center" Height="36" Width="90" Grid.Column="1" Click="ButtonBase_OnClick"/>
    </Grid>
</Window>
